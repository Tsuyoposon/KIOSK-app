<p id="notice"><%= notice %></p>

<h1>Listing Wents</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Shop</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wents.each do |went| %>
      <tr>
        <td><%= went.user_id %></td>
        <td><%= went.shop_id %></td>
        <td><%= link_to 'Show', went %></td>
        <td><%= link_to 'Edit', edit_went_path(went) %></td>
        <td><%= link_to 'Destroy', went, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Went', new_went_path %>
